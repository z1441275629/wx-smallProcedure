.goodsbox{
  width: 100%;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  background-color: #e7e5dc;
}
.goodslist{
  width: 45%;
  height: 400rpx;
  background-color: #fff;
  margin-top: 20rpx;
}
.goodslist view{
  width: 100%;
  height: 200rpx;
}
.goodslist view image{
  width: 100%;
  height: 100%;
}
.shopTitle{
  font-size: 25rpx;
  font-weight: 600;
  width: 100%;
}
.shopPrice{
  color: red;
}